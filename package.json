{
	"name": "@svelte-kits/store",
	"type": "module",
	"version": "0.1.1",
	"license": "MIT",
	"description": "Safe stores for SvelteKit",
	"homepage": "https://github.com/svelte-kits/store#readme",
	"author": "Bae Junehyeon <devunt@gmail.com>",
	"repository": {
		"type": "git",
		"url": "https://github.com/svelte-kits/store.git"
	},
	"keywords": [
		"svelte",
		"sveltekit",
		"svelte-kit"
	],
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"scripts": {
		"install-hooks": "husky install",
		"bootstrap": "svelte-kit sync",
		"dev": "vite dev",
		"check": "svelte-check --fail-on-warnings",
		"lint": "eslint --max-warnings=0 .",
		"format": "eslint --fix .",
		"package": "svelte-kit sync && svelte-package && publint"
	},
	"peerDependencies": {
		"@sveltejs/kit": "^1.0.0",
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.15.5",
		"@sveltejs/package": "^2.0.2",
		"@typescript-eslint/eslint-plugin": "^5.58.0",
		"eslint": "^8.38.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"eslint-plugin-unicorn": "^46.0.0",
		"gitmoji-cli": "^8.1.1",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.1",
		"prettier": "^2.8.7",
		"publint": "^0.1.11",
		"svelte": "^4.0.0",
		"svelte-check": "^3.2.0",
		"typescript": "^5.0.4",
		"vite": "^4.2.1"
	}
}
